{
  "name": "@vgrid/clappr-rtmp",
  "version": "1.1.1",
  "description": "RTMP Support for Clappr Player",
  "main": "dist/rtmp.min.js",
  "author": "Fl√°vio Ribeiro",
  "keywords": [
    "rtmp",
    "clappr",
    "player",
    "online video",
    "streaming"
  ],
  "homepage": "http://clappr.io",
  "bugs": {
    "url": "http://github.com/vgrid/clappr-rtmp-plugin"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vgrid/clappr-rtmp-plugin.git"
  },
  "scripts": {
    "build_swf": "sh build_player.sh",
    "release": "NODE_ENV=production node_modules/.bin/webpack --progress -d --output-filename rtmp.min.js",
    "build": "node_modules/.bin/webpack --progress",
    "watch": "node_modules/.bin/webpack --progress --watch",
    "test": "karma start --single-run --browsers Chrome",
    "start": "node_modules/.bin/webpack-dev-server --content-base public/ --output-public-path /latest --hot",
    "lock": "rm -rf npm-shrinkwrap.json node_modules && npm install --silent && npm shrinkwrap"
  },
  "dependencies": {
    "clappr": "latest",
    "html-loader": "^0.5.5",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "mocha": "^5.2.0",
    "mocha-loader": "^2.0.0",
    "node-sass": "^4.9.3",
    "style-loader": "^0.23.0",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  }
}
